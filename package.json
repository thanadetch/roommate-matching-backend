{
  "name": "roommate-matching-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:api-gateway": "nest start api-gateway --watch",
    "start:profiles": "nest start profiles --watch",
    "prisma:profiles:generate": "dotenv -e apps/profiles/.env -- prisma generate --schema=./apps/profiles/prisma/schema.prisma",
    "prisma:profiles:migrate": "dotenv -e apps/profiles/.env -- prisma migrate dev --schema=./apps/profiles/prisma/schema.prisma",
    "start:rooms": "nest start rooms --watch",
    "prisma:rooms:generate": "dotenv -e apps/rooms/.env -- prisma generate --schema=./apps/rooms/prisma/schema.prisma",
    "prisma:rooms:migrate": "dotenv -e apps/rooms/.env -- prisma migrate dev --schema=./apps/rooms/prisma/schema.prisma",
    "start:notifications": "nest start notifications --watch",
    "prisma:notifications:generate": "dotenv -e apps/notifications/.env -- prisma generate --schema=./apps/notifications/prisma/schema.prisma",
    "prisma:notifications:push": "dotenv -e apps/notifications/.env -- prisma db push --schema=./apps/notifications/prisma/schema.prisma",
    "start:reviews": "nest start reviews --watch",
    "prisma:reviews:generate": "dotenv -e apps/reviews/.env -- prisma generate --schema=./apps/reviews/prisma/schema.prisma",
    "prisma:reviews:migrate": "dotenv -e apps/reviews/.env -- prisma migrate dev --schema=./apps/reviews/prisma/schema.prisma",
    "start:roommate-matching": "nest start roommate-matching --watch",
    "prisma:roommate-matching:generate": "dotenv -e apps/roommate-matching/.env -- prisma generate --schema=./apps/roommate-matching/prisma/schema.prisma",
    "prisma:roommate-matching:migrate": "dotenv -e apps/roommate-matching/.env -- prisma migrate dev --schema=./apps/roommate-matching/prisma/schema.prisma",
    "start:auth": "nest start auth --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/roommate-matching-backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/roommate-matching-backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@grpc/reflection": "^1.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.16.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/photos(|/.*)$": "<rootDir>/libs/photos/src/$1"
    }
  }
}
